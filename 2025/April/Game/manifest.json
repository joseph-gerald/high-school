{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "manifest_version": 3,
  "version": "1.0",
  "name": "auto-norstat-survey",
  "description": "A minimum extension template. This template includes a manifest file.",
  "author": "Your Name",
  "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./scripts/content-script.js"]
    }
  ]
}